.container{background-color:#f7f7f7;height:100%}.container .avatar-info{padding:0 20px}.container .avatar-info .avatar-img{width:64px;height:64px}.container .avatar-info .user-phone{padding-top:10px;padding-left:20px}.container .avatar-info .user-phone .login-btn{background-color:#4674f6;border:none}.container .avatar-info .user-phone .phone{font-size:20px;font-weight:600}.container .avatar-info .user-phone .id-num{color:#666}.container .icon-List{display:flex;flex-wrap:nowrap;flex-direction:column;align-items:center;justify-content:center}.container .grid-item-box{border:none;flex:1;display:flex;flex-direction:column;align-items:center;justify-content:center;padding:15px 0}.container .grid-item-box .icon-img{width:42px;height:42px}.container .grid-item-box .text{font-size:14px;margin-top:10px}.container .item-vip{padding:20px;position:relative}.container .item-vip .img-vip{width:100%;height:108px}.container .item-vip .vip-text{position:absolute;z-index:10;padding-top:35rpx;padding-left:20rpx;color:#9f6020}.container .item-vip .vip-text-down{position:absolute;z-index:10;padding-top:90rpx;padding-left:20rpx;color:#9f6020}.container .nav-list{padding:0 20px}.container .nav-list .nav-item{height:50px;display:flex;flex-wrap:nowrap;align-items:center}.container .nav-list .nav-item .nav-icon{width:50px;padding-top:5px}.container .nav-list .nav-item .nav-icon .nav-img{width:24px;height:24px}.container .nav-list .nav-item .nav-box{display:flex;justify-content:space-between;width:calc(100vh - 200px);height:40px;line-height:40px;margin-left:15px;padding-left:5px;border-bottom:1px solid #c2c2c2!important}
