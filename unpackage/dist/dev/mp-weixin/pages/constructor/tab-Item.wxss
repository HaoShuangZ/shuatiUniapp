.container{height:75vh;background-color:#fff}.shareView{margin-top:60px;width:100px;height:40px;line-height:40px;text-align:center;background-color:#faebd7;align-self:center}.content{display:flex;background-color:pink;margin-bottom:20rpx}.content .item{display:flex;flex-direction:column;width:25%;padding:0 38rpx;font-size:16rpx;align-items:center;justify-content:center;box-sizing:border-box;height:150rpx;background-color:#fff}.content .icon{margin-bottom:10rpx}.content .icon .iconfont{color:#4674f6!important;font-size:50rpx!important}.content .nav{font-size:26rpx;margin-bottom:10rpx}.content .num{font-size:23rpx}.tab_nav{display:flex;justify-content:center;align-items:center;background:#fff}.tab_nav:after{position:absolute;content:"";width:200%;height:200%;border:1px solid red;transform-origin:0 0;transform:scale(.5)}.tab_nav .navTitle{height:90rpx;line-height:90rpx;width:100%;text-align:center;font-size:32rpx;font-family:Alibaba PuHuiTi;color:#333}.active{position:relative;color:#4674f6}.active:after{content:"";position:absolute;width:100rpx;height:8rpx;background-color:#4674f6;left:0px;right:0px;bottom:0px;margin:auto}.list-content{background-color:#fff;height:calc(100vh - 266px)}.list-content .list-box{display:flex;flex-wrap:wrap;background-color:#fff;padding:40rpx 10rpx 0}.list-content .list-box .list{width:50%;padding:0 10rpx;box-sizing:border-box;height:130rpx}.list-content .list-box .list .exercise-list{display:flex;justify-content:space-between;align-items:center;width:100%;background-color:#f3f8fe;box-sizing:border-box;padding:20rpx;border-radius:15rpx;margin-bottom:20rpx}.list-content .list-box .list .exercise-list .title{color:#f15752}.list-content .list-box .list .exercise-list .icon-img{width:50rpx;height:50rpx}.list-content .list-box .list .exercise-list .desc{font-size:20rpx;color:#999}.share-btn{display:flex;border-top:1px solid #f8f8f8;padding-top:20rpx}.share-btn .button-com{height:75rpx;line-height:75rpx;border-radius:35rpx;color:#fff}.share-btn .share{background-color:#58be68}.share-btn .collect{background-color:#4674f6}
